# Environment variables for Ledgerly - Example File
# Copy this file to .env and fill in your actual values

# MetaMask/Infura API Configuration
# Replace with your actual HTTPS RPC URL from MetaMask/Infura
ETHEREUM_RPC_URL=https://sepolia.infura.io/v3/your_project_id_here
ETHEREUM_API_KEY=your_api_key_here

# Network Configuration
DEFAULT_NETWORK=ethereum_sepolia
ENABLE_MAINNET=false

# Development Settings  
DEBUG_MODE=true
